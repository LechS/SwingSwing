{% extends 'base.html.twig' %}

{% block body %}
    <h1>FbEndpoint</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fbEndpoint.id }}</td>
            </tr>
            <tr>
                <th>Fbid</th>
                <td>{{ fbEndpoint.fbId }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ fbEndpoint.name }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ fbEndpoint.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ fbEndpoint.country }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ fbEndpoint.type }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('fbendpoint_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('fbendpoint_edit', { 'id': fbEndpoint.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
